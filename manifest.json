{
  "manifest_version": 3,
  "name": "WJ AutoLogin Pro",
  "version": "2.0",
  "description": "Puente de autologueo din√°mico para WJ ERP",
  "permissions": [
    "cookies",
    "scripting",
    "tabs",
    "storage" 
  ],
  "optional_host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "host_permissions": [
    "https://*.sunat.gob.pe/*",
    "https://*.afpnet.com.pe/*"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "background": {
    "service_worker": "background.js"
  }
}